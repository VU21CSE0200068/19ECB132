#include<stdio.h>
#define SIZE 10
intmain(void) {
  int a[SIZE] = {1,2,3,5,6,7,8,9};
  for (int i=0; i<SIZE; i++) 
    printf("%d \t",a[i] );
  a[10] = 25;
  printf("\nafter inserting...\n");
  for (int i=0; i<SIZE+1; i++) 
    printf("%d \t",a[i] );
}
/* Output
a+=5 is 9
 a-=6 is 3
 a*=7 is 21
 a%=2 is 1
 a/=3 is 0
*/
